<p id="notice"><%= notice %></p>

<h1>Listing Creates</h1>

<table>
  <thead>
    <tr>
      <th>Character</th>
      <th>Droid</th>
      <th>Planet</th>
      <th>Quote</th>
      <th>Species</th>
      <th>Vehicle</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @creates.each do |create| %>
      <tr>
        <td><%= create.character %></td>
        <td><%= create.droid %></td>
        <td><%= create.planet %></td>
        <td><%= create.quote %></td>
        <td><%= create.species %></td>
        <td><%= create.vehicle %></td>
        <td><%= link_to 'Show', create %></td>
        <td><%= link_to 'Edit', edit_create_path(create) %></td>
        <td><%= link_to 'Destroy', create, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Create', new_create_path %>
